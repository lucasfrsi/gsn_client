{
  "name": "gsn_client",
  "version": "1.0.0",
  "description": "GamersX client",
  "main": "src/index.jsx",
  "scripts": {
    "start": "webpack serve --config ./webpack.config.js --progress",
    "build": "npm run lint && cross-env NODE_ENV=production webpack --config webpack.config.js --progress",
    "build:nocheck": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress",
    "serve": "http-server ./dist -g",
    "clean": "rm -rf node_modules && rm -rf dist",
    "lint": "eslint --ext .jsx,.js ./"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasfrsi/gsn_client"
  },
  "keywords": [
    "axios",
    "jsx",
    "prop-types",
    "react",
    "redux",
    "redux-saga",
    "router",
    "webpack"
  ],
  "author": "Lucas F. Simoni",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^7.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.2",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fibers": "^5.0.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.3.9",
    "postcss": "^8.2.13",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^1.3.0",
    "terser-webpack-plugin": "^3.1.0",
    "webpack": "^5.27.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.9.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
